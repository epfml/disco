{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "run_server.js",
  "scripts": {
    "dev": "nodemon src/run_server.ts",
    "start": "NODE_PATH=./dist node dist/run_server.js",
    "build": "tsc -p tsconfig.prod.json",
    "clean": "tsc --build --clean && rm -r dist || true",
    "test": "mocha --exit -r ts-node/register 'tests/**/*.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow-models/mobilenet": "2",
    "@tensorflow/tfjs-node": "3",
    "cors": "2",
    "express": "4",
    "express-ws": "5",
    "immutable": "4",
    "lodash": "4",
    "msgpack-lite": "0.1",
    "typescript": "4",
    "ws": "8"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "models/*"
    ],
    "execMap": {
      "ts": "node --inspect=0.0.0.0:9229 --nolazy -r ts-node/register"
    }
  },
  "devDependencies": {
    "@types/chai": "4",
    "@types/cors": "2",
    "@types/express-ws": "3",
    "@types/mocha": "9",
    "@types/ws": "8",
    "chai": "4",
    "mocha": "9",
    "nodemon": "2",
    "supertest": "6",
    "ts-node": "10",
    "ts-node-register": "1"
  }
}
