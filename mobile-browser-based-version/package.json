{
  "name": "mobile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --exit -r ts-node/register 'tests/**/*.ts'",
    "build17": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "lint17": "export NODE_OPTIONS=--openssl-legacy-provider  && vue-cli-service lint",
    "serve17": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve"
  },
  "dependencies": {
    "@meforma/vue-toaster": "^1.2.2",
    "@tensorflow/tfjs": "^3.13.0",
    "apexcharts": "^3.31.0",
    "axios": "^0.25.0",
    "d3": "^6.7.0",
    "jshashes": "^1.0.8",
    "lodash": "^4.17.21",
    "msgpack-lite": "^0.1.26",
    "node-fetch": "^2.6.7",
    "papaparse": "^5.3.1",
    "peerjs": "^1.3.2",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.3.1",
    "ts-node-register": "^1.0.0",
    "vee-validate": "^4.5.7",
    "vue": "^3.2.26",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.11",
    "vue3-apexcharts": "^1.4.1",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.4",
    "@types/chai": "^4.3.0",
    "@types/d3": "^7.1.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.3.0",
    "mocha": "^9.2.0",
    "postcss": "^8.4.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "typescript": "^4.1.6",
    "vue-loader-v16": "npm:vue-loader@^16.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "multi-word-component-names": "off",
      "vue/multi-word-component-names": "off",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
