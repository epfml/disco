    "eslint": "7",
    "eslint-config-standard-with-typescript": "21",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "5",
    "mocha": "9",
    "nodemon": "2",
    "rand-seed": "^1.0.1",
